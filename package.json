    "@typescript-eslint/parser": "^5.55.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.36.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "lefthook": "^1.4.8",
    "prettier": "^3.0.1",
